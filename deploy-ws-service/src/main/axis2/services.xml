<?xml version="1.0" encoding="UTF-8"?>
<service name="DeploymentService" scope="application">
    <description>Tempo Deployment Service</description>
    <operation name="deployAssembly">
        <messageReceiver class="org.apache.axis2.receivers.RawXMLINOutMessageReceiver"/>
    </operation>
    <operation name="undeployAssembly">
        <messageReceiver class="org.apache.axis2.receivers.RawXMLINOutMessageReceiver"/>
    </operation>
    <operation name="getDeployedAssemblies">
        <messageReceiver class="org.apache.axis2.receivers.RawXMLINOutMessageReceiver"/>
    </operation>
    <parameter name="ServiceClass">org.intalio.tempo.deployment.ws.DeployWS</parameter>
</service>

