<xs:schema targetNamespace="http://www.intalio.com/examples/AbsenceRequest"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    elementFormDefault="qualified">

    <xs:element name="evaluation">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="accept" type="xs:boolean"/>
                <xs:element name="reason" type="xs:string"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

</xs:schema>
