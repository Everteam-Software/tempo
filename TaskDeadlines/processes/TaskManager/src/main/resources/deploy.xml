<?xml version="1.0" encoding="UTF-8"?>
<deploy xmlns="http://ode.fivesight.com/schemas/2006/06/27/dd" 
        xmlns:process="http://www.intalio.com/bpms/workflow/ib4p_20051115"
        xmlns:sns="http://www.intalio.com/bpms/workflow/ib4p_20051115"
	xmlns:tms="http://www.intalio.com/BPMS/Workflow/TaskManagementServices-20051109/">

	<process name="process:TaskManager">
		<active>true</active>
        
        <!-- userProcessPartnerLink -->
		<provide partnerLink="userProcessPartnerLink">
			<service name="sns:createTaskService" port="userProcessPort"/>
		</provide>
		<invoke partnerLink="userProcessPartnerLink">
			<service name="sns:notifyTaskCompletionService" port="userProcessPort"/>
		</invoke>
        
        <!-- uifwPartnerLink -->
		<provide partnerLink="uifwPartnerLink">
			<service name="sns:completeTaskService" port="uifwPort"/>
		</provide>
        
        <!-- TMSPartnerLink -->
		<invoke partnerLink="TMSPartnerLink">
			<service name="tms:TaskManagementServices" port="TaskManagementServicesSOAP"/>
		</invoke>
        
        <!-- taskManagerPartnerLink -->
		<provide partnerLink="nextTaskManagerPartnerLink">
			<service name="sns:nextTaskReadyService" port="taskManagerPort"/>
		</provide>
		<invoke partnerLink="previousTaskManagerPartnerLink">
			<service name="sns:nextTaskReadyService" port="taskManagerPort"/>
		</invoke>
        
	</process>
    
</deploy>
