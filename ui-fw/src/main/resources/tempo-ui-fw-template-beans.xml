<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN"
          "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
 <bean id="abs.AbstractJPAConnectionFactory" class="org.intalio.tempo.workflow.dao.AbstractJPAConnectionFactory" abstract="true">
	 <constructor-arg index="0" type="java.lang.String" value="org.intalio.tempo.tms">
