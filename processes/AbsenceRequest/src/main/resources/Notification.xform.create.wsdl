<wsdl:definitions
  targetNamespace="http://www.intalio.com/workflow/forms/AbsenceRequest/Notification"
  xmlns="http://schemas.xmlsoap.org/wsdl/"
  xmlns:ar="http://www.intalio.com/workflow/forms/AbsenceRequest/Notification"
  xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
  xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <wsdl:types>
    <xsd:schema elementFormDefault="qualified" targetNamespace="http://www.intalio.com/workflow/forms/AbsenceRequest/Notification">
      <xsd:include schemaLocation="Notification.xform.create.xsd"/>
    </xsd:schema>
  </wsdl:types>
  <!-- Messages -->
  <wsdl:message name="createTaskRequest">
    <wsdl:part element="ar:createTaskRequest" name="root"/>
  </wsdl:message>
  <wsdl:message name="createTaskResponse">
    <wsdl:part element="ar:response" name="root"/>
  </wsdl:message>
  <!-- Port Types -->
  <wsdl:portType name="FDSPortType">
    <wsdl:operation name="createTask">
      <wsdl:input message="ar:createTaskRequest" name="createTaskRequest"/>
      <wsdl:output message="ar:createTaskResponse" name="createTaskResponse"/>
    </wsdl:operation>
  </wsdl:portType>
  <!-- SOAP Bindings -->
  <wsdl:binding name="FDSSoapBinding" type="ar:FDSPortType">
    <soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>
    <wsdl:operation name="createTask">
      <soap:operation soapAction="createTask" style="document"/>
      <wsdl:input>
        <soap:body
          namespace="http://www.intalio.com/workflow/forms/AbsenceRequest/Notification" use="literal"/>
      </wsdl:input>
      <wsdl:output>
        <soap:body
          namespace="http://www.intalio.com/workflow/forms/AbsenceRequest/Notification" use="literal"/>
      </wsdl:output>
    </wsdl:operation>
  </wsdl:binding>
  <!-- Services -->
  <wsdl:service name="createTaskService">
    <wsdl:port binding="ar:FDSSoapBinding" name="FDSPort">
      <soap:address location="http://localhost:8080/fds/activity"/>
    </wsdl:port>
  </wsdl:service>
</wsdl:definitions>
